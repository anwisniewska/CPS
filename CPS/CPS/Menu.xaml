            <ComboBoxItem IsSelected="True">+</ComboBoxItem>
            <ComboBoxItem>-</ComboBoxItem>
            <ComboBoxItem>*</ComboBoxItem>
            <ComboBoxItem>/</ComboBoxItem>
            <ComboBoxItem>splot</ComboBoxItem>
            <ComboBoxItem>korelacja bezpośrednia</ComboBoxItem>
            <ComboBoxItem>korelacja z użyciem splotu</ComboBoxItem>
        </ComboBox>
        <Button Content="y" HorizontalAlignment="Left" Margin="170,331,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click_Y" Height="19"/>
        <Button Content="wynik" HorizontalAlignment="Left" Margin="250,331,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click_wynik" Height="19"/>
