        <TextBlock Grid.Column="2" HorizontalAlignment="Left" Margin="137.4,21,0,0" TextWrapping="Wrap" Text="numer pierwszej próbki" VerticalAlignment="Top" RenderTransformOrigin="-0.276,0.677" Height="16" Width="124"/>
