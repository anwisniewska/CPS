        <TextBox Text="{Binding _T}" HorizontalAlignment="Left" Height="23" Margin="12,98,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <TextBox Text="{Binding _kw}" HorizontalAlignment="Left" Height="23" Margin="12,126,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Button Content="Szum o rozkładzie jednostajnym" HorizontalAlignment="Left" Margin="12,214,0,0" VerticalAlignment="Top" Width="298" Click="Button_Click_szumjedno" Height="19"/>
        <TextBlock HorizontalAlignment="Left" Margin="137,21,0,0" TextWrapping="Wrap" Text="Amplituda" VerticalAlignment="Top" Height="16" Width="55"/>
