            <ComboBoxItem>Sygnał sinusoidalny wyprostowany dwupołówkowo</ComboBoxItem>
            <ComboBoxItem>Sygnał prostokątny</ComboBoxItem>
            <ComboBoxItem>Sygnał prostokątny symetryczny</ComboBoxItem>
            <ComboBoxItem>Sygnał trójkątny</ComboBoxItem>
            <ComboBoxItem>Skok jednostkowy</ComboBoxItem>
            <ComboBoxItem>Impuls jednostkowy</ComboBoxItem>
            <ComboBoxItem>Szum impulsowy</ComboBoxItem>
        </ComboBox>
        <Button Content="Stwórz sygnał" HorizontalAlignment="Left" Margin="328,10,0,0" VerticalAlignment="Top" Width="97" Click="Button_Click_stworz" Height="22"/>
        <ComboBox Name="kwantyzacja" HorizontalAlignment="Left" Margin="10,375,0,0" VerticalAlignment="Top" Width="313">
            <ComboBoxItem IsSelected="True">Kwantyzacja z obciecięm</ComboBoxItem>
            <ComboBoxItem>Kwantyzacja z zaokrągleniem</ComboBoxItem>
        </ComboBox>
        <ComboBox Name ="odtwarzanie" HorizontalAlignment="Left" Margin="10,402,0,0" VerticalAlignment="Top" Width="313">
            <ComboBoxItem IsSelected="True">Ekstrapolacja zerowego rzędu</ComboBoxItem>
            <ComboBoxItem>Interpolacja pierwszego rzędu</ComboBoxItem>
        </ComboBox>
        <Button Content="x" HorizontalAlignment="Left" Margin="9,331,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click_X" Height="19"/>
        <Button Content="y" HorizontalAlignment="Left" Margin="170,331,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click_Y" Height="19"/>
        <TextBlock HorizontalAlignment="Left" Margin="10,450,0,0" TextWrapping="Wrap" Text="Filtracja" VerticalAlignment="Top"/>
        <TextBox Text="{Binding _M}" HorizontalAlignment="Left" Height="23" Margin="9,498,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <TextBox Text="{Binding _K}" HorizontalAlignment="Left" Height="23" Margin="9,471,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Width="120"/>
        <TextBlock HorizontalAlignment="Left" Margin="136,505,0,0" TextWrapping="Wrap" Text="M" VerticalAlignment="Top"/>
        <TextBlock HorizontalAlignment="Left" Margin="136,478,0,0" TextWrapping="Wrap" Text="K" VerticalAlignment="Top"/>
        <ComboBox Name="okienko" HorizontalAlignment="Left" Margin="10,553,0,0" VerticalAlignment="Top" Width="313">
            <ComboBoxItem IsSelected="True">Okno Prostokątne</ComboBoxItem>
            <ComboBoxItem>Okno Hamminga</ComboBoxItem>
            <ComboBoxItem>Okno Hanninga</ComboBoxItem>
            <ComboBoxItem>Okno Blackmana</ComboBoxItem>
        </ComboBox>
        <ComboBox Name="typFiltru" HorizontalAlignment="Left" Margin="10,580,0,0" VerticalAlignment="Top" Width="313">
            <ComboBoxItem IsSelected="True">Filtr dolnoprzepustowy</ComboBoxItem>
            <ComboBoxItem>Filtr środkowoprzepustowy</ComboBoxItem>
            <ComboBoxItem>Filtr górnoprzepustowy</ComboBoxItem>
        </ComboBox>
        <TextBlock HorizontalAlignment="Left" Margin="10,622,0,0" TextWrapping="Wrap" Text="Radar" VerticalAlignment="Top"/>
        <TextBox Text="{Binding _Opoznienie}" HorizontalAlignment="Left" Height="23" Margin="10,644,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Width="120"/>
        <TextBlock HorizontalAlignment="Left" Margin="138,651,0,0" TextWrapping="Wrap" VerticalAlignment="Top"><Run Text="opó"/><Run Text="źnienie (w próbkach)"/></TextBlock>
    </Grid>
</Window>
