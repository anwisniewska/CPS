        <Button Content="Sygnał prostokątny symetryczny" HorizontalAlignment="Left" Margin="12,358,0,0" VerticalAlignment="Top" Width="298" Height="20" Click="Button_Click_sygprosym"/>
        <Button Content="Sygnał trójkątny" HorizontalAlignment="Left" Margin="12,382,0,0" VerticalAlignment="Top" Width="298" Height="20" Click="Button_Click_sygtroj"/>
        <Button Content="Skok jednostkowy" HorizontalAlignment="Left" Margin="12,406,0,0" VerticalAlignment="Top" Width="298" Height="20" Click="Button_Click_skok"/>
